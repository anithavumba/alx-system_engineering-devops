frontend	anitha-frontend
        bind *:80
        mode http
        timeout client 30s
        default_backend anitha-backend
# Add a frontend to handle incoming HTTPS connections
frontend anitha-https
        # Bind 443 with the generated letsencrypt cert.
        bind *:443 ssl crt /etc/letsencrypt/live/theautoapp.tech/fullchain.pem
        # Select a Challenge
        acl letsencrypt-acl path_beg /.well-known/acme-challenge/
        # Use the challenge backend if the challenge is set
        use_backend letsencrypt-backend if letsencrypt-acl
        timeout client 30s
        default_backend anitha-backend

backend anitha-backend
        mode http
        balance roundrobin
        server 110961-web-01 35.153.50.195:80 check
        server 110961-web-02 100.25.103.37:80 check
        timeout server 30s
        timeout connect 5s

backend letsencrypt-backend
        # Lets encrypt backend server
        server letsencrypt 127.0.0.1:54321
        timeout server 30s
        timeout connect 30s
        
global
        log /dev/log    local0
        log /dev/log    local1 notice
        chroot /var/lib/haproxy
        stats socket /run/haproxy/admin.sock mode 660 level admin expose-fd listeners
        stats timeout 30s
        user haproxy
        group haproxy
        daemon

        # Default SSL material locations
        ca-base /etc/ssl/certs
        crt-base /etc/ssl/private

        # See: https://ssl-config.mozilla.org/#server=haproxy&server-version=2.0.3&config=intermediate
        ssl-default-bind-ciphers ECDHE-ECDSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-CHACHA20-POLY1305:ECDHE-RSA-CHACHA20-POLY1305:DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384
        ssl-default-bind-ciphersuites TLS_AES_128_GCM_SHA256:TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256
        ssl-default-bind-options ssl-min-ver TLSv1.2 no-tls-tickets

defaults
        log     global
        mode    http
        option  httplog
        option  dontlognull
        timeout connect 5000
        timeout client  50000
        timeout server  50000
        errorfile 400 /etc/haproxy/errors/400.http
        errorfile 403 /etc/haproxy/errors/403.http
        errorfile 408 /etc/haproxy/errors/408.http
        errorfile 500 /etc/haproxy/errors/500.http
        errorfile 502 /etc/haproxy/errors/502.http
        errorfile 503 /etc/haproxy/errors/503.http
        errorfile 504 /etc/haproxy/errors/504.http



# start of parameters for theautoapp.tech
frontend theautoapp.tech-http-frontend
        bind     *:80
        redirect scheme https code 301 if !{ ssl_fc }
        http-request set-header X-Forwarded-Proto http
        default_backend  theautoapp.tech-backend
frontend b3zaleel-alx.tech-https-frontend
        bind *:443 ssl crt /etc/letsencrypt/live/www.theautoapp.tech/fullchain.pem
        http-request set-header X-Forwarded-Proto https
        default_backend theautoapp.tech-backend
backend theautoapp.tech-backend
        balance roundrobin
        server 17272-web-01 34.207.253.142:80 check
        server 17272-web-02 100.25.103.37:80 check
# end of parameters
